{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": ["prettier"],
  "plugins": ["prettier"],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "rules": {
    "prettier/prettier": "off",
    "no-unused-vars": "warn",
    "func-names": "off"
  }
}
